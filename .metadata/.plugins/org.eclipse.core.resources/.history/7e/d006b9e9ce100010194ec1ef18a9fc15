{*Escribir un programa que detecte el nombre introducido por el usuario.*}
program case1;
var 
	nombre: array[1..30] of char;
	i,j: integer;
begin
	i:=0;
	write('Ingrese su nombre: ');
	repeat
	i:=i+1;
	read(nombre[i]);
	until eoln;
	write('Su nombr es ');
	for j:=1 to i do
		write(nombre[j]);
end.
