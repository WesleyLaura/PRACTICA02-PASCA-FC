{*Escribir un programa que dado un número del 1 al 12 escriba el correspondiente nombre
del mes del año.*}
program case3;
var
	numero: integer;
begin
	write('Ingrese un numero: ');
	readln(numero);
	if (numero>=1) and (numero<=12) then
		case numero of
			1: writeln('Enero');
			2: writeln('Febrero');
			3: writeln('Marzo');
			4: writeln('Abril');
			5: writeln('Mayo');
			6: writeln('Junio');
			7: writeln('Julio');
			8: writeln('Agosto');
			9: writeln('Septiembre');
			10: writeln('Octubre');
			11: writeln('Noviembre');
			12: writeln('Diciembre');
		end
	else
		writeln('Ingrese un numero valido');
	readln;
end.